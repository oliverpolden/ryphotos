{#
/**
 * @file
 * Template for the Photo Grid style.
 *
 * - $items contains an array of images to display.
 *
 */
#}
{% if title %}
  <h3>{{ title }}</h3>
{% endif %}

<div class="views-photo-grid-container">
  {% for item in items %}
    <div class="views-photo-grid-item">{{  item }}</div>
  {% endfor %}
</div>

